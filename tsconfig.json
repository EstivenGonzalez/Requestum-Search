{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext"],
    "types": ["@types/styled-components-react-native"],
    "jsx": "react-native",
    "noEmit": true,
    "isolatedModules": true,
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@screens/*": ["screens/*"],
      "@modules/*": ["modules/*"],
      "@services/*": ["services/*"],
      "@hooks/*": ["hooks/*"],
      "@store/*": ["store/*"],
      "@theme/*": ["theme/*"],
      "@typings/*": ["typings/*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": false
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js"]
}
